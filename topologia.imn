node n2 {
    type router
    model mdr
    p2P-config {
	hostname n2
	!
	interface eth0
	 ip address 10.0.0.1/32
	 ipv6 address 2001:0::1/128
	!
    }
    canvas c1
    iconcoords {312.0 180.0}
    labelcoords {312.0 212.0}
    services {OSPFv3MDR zebra DHCPClient IPForward}
    interface-peer {eth0 n1}
}

